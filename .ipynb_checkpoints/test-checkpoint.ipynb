   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "shape = (50, 4460)\n",
    "\n",
    "data = np.random.normal(size=shape)\n",
    "\n",
    "data[:, 1000] += data[:, 2000]\n",
    "\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "c = df.corr().abs()\n",
    "\n",
    "s = c.unstack()\n",
    "so = s.order(kind=\"quicksort\")\n",
    "\n",
    "print so[-4470:-4460]"
   ]
